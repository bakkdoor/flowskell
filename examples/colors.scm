(define (list . objs)  objs)
(define dim 6)
(define col
  (lambda (n r)
    (translate 1.0 0.0 0.0)
    (color (list (/ (sin (/ (secs) 10.0)) 1.0) (cos (/ n 10.0)) (- 0.3 (/ (sin (* (/ (secs) 8.0) n)) 3.0))))
    (push)
        (translate 0.0 0.0 (- 0.3 (/ (sin (* (/ (secs) 8.0) n)) 3.0)))
        (scale 0.4 0.4 0.4)
        (make-cube)
    (pop)
    (if (> n 0) (col (- n 1) r))
  )
)
(define row
  (lambda (n)
    (translate 0.0 1.0 0.0)
    (push)
    (col dim n)
    (pop)
    (if (> n 0) (row (- n 1)))
  )
)
(define every-frame
  (lambda ()
    (rotate 30.0 1.0 -0.2 0.0)
    (scale 0.1 0.1 0.1)
    (translate (/ dim -2.0) (/ dim -2.0) 0.0)
    (row dim)))
