(define every-frame
  (cons
    (rotate 90.0 0.0 0.0 1.0)
    (rotate 45.0 0.0 1.0 0.0)
    (rotate (modulo (/ (msecs) 10) 360) 1.0 0.0 0.0)
    (scale 0.5 0.5 0.5)
    (make-cube)
    (translate 1.0 0.0 0.0)
    (scale 0.5 0.5 0.5)
    (make-cube)
    (translate 1.0 0.0 0.0)
    (scale 0.5 0.5 0.5)
    (make-cube)
    (translate 1.0 0.0 0.0)
    (scale 0.5 0.5 0.5)
    (make-cube)
    (translate 1.0 0.0 0.0)
    (scale 0.5 0.5 0.5)
    (make-cube)))
